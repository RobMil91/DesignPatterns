package teaExample;

public class testTemplateMethod {

	
	public static void main(String[] args) {
		
		
		System.out.println("make a tea");
		Tea tea = new Tea();
		
		//call the entire Template Method
		tea.prepareRecipe();
		
		
		System.out.println("make a coffee");
		Coffee cof = new Coffee();
		
		cof.prepareRecipe();
	}
}
