package composition;

public abstract class MenuComponent {

	
	public void add(MenuComponent menuComp) {
		
		throw new UnsupportedOperationException("");
	}
	
	public void remove(MenuComponent menuComp) {
		
		throw new UnsupportedOperationException("");
	}
	
	public void getChild(MenuComponent menuComp) {
		
		throw new UnsupportedOperationException("");
	}
	
	public void getName(MenuComponent menuComp) {
		
		throw new UnsupportedOperationException("");
	}
	
	public void getDescription(MenuComponent menuComp) {
		
		throw new UnsupportedOperationException("");
	}
	
	
}
